{
  "env": {
    "browser": true,
    "es6": true,
    "webextensions": true,
    "jest": true
  },
  "extends": [
    "standard",
    "plugin:jsdoc/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "jsdoc"
  ],
  "rules": {}
}
