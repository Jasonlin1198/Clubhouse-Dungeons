{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Quaranteam-8 Chrome Extension",
  "description": "Quaranteam-8 Project Description",
  "icons": {
    "16": "images/quaranteam8logo16.png",
    "48": "images/quaranteam8logo48.png",
    "128": "images/quaranteam8logo128.png"
  },
  "browser_action": {
    "default_icon": "images/quaranteam8logo.png",
    "default_popup": "login.html",
    "default_title": "Quaranteam-8"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://*.quaranteam-08.firebaseio.com; object-src 'self'",
  "key": ""
}